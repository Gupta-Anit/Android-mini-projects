<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- English resources -->

    <!-- Timed action hour/time display using String.format, see the
         Date and Time section at
           http://java.sun.com/j2se/1.5.0/docs/api/java/util/Formatter.html
         Summary:
           tk=hour 0-23, tl=hour 1-12, tp=am/om, tM=minutes 00-59
     -->
    <!-- display time on exact hour -->
    <string name="timedaction_time_0_minute">%1$tl %1$tp</string>
    <!-- display time: hour with minutes != 0, tM=minutes 00-59 -->
    <string name="timedaction_time_with_minutes">%1$tl:%1$tM %1$tp</string>

    <!-- toast_next_alarm_date_time uses Java SimpleDateFormat, see
         http://java.sun.com/j2se/1.5.0/docs/api/java/text/SimpleDateFormat.html
    -->
    <string name="toast_next_alarm_date_time">MM/dd/yyyy HH:mm:ss</string>

</resources>
